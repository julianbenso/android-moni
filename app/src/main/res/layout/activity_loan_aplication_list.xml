<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >

    <TextView
        android:id="@+id/title_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="50dp"
        android:text="@string/title_lista_de_solicitudes_de_prestamo"
        android:textColor="@color/purple_500"
        android:textSize="19sp"
        android:textStyle="bold"
        android:layout_marginVertical="5dp"
        />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/container_loan_aplication_list"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title_list"
        android:layout_marginTop="5dp"
        android:name="com.moni.prestamomoni.presentation.LoanAplicationListFragment"
        />

</androidx.constraintlayout.widget.ConstraintLayout>